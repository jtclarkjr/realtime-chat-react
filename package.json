{
  "name": "realtime-chat-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun run --bun next dev",
    "dev:instance1": "bun run --bun next dev -p 3000",
    "dev:instance2": "bun run --bun next dev -p 3001",
    "build": "bun run --bun next build",
    "build:app": "next build",
    "start": "next start",
    "lint": "eslint && tsc --noEmit",
    "prettier": "prettier --write .",
    "docker:up": "docker compose up --build -d",
    "docker:down": "docker compose down -v",
    "docker:logs": "docker compose logs -f redis",
    "docker:debug": "docker compose --profile debug up -d",
    "dev:docker": "docker compose up -d && bun dev",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-storybook-local": "storybook build && rm -rf public/storybook && mv storybook-static public/storybook",
    "supabase:ssr:build": "tsc -p tsconfig.supabase-ssr.module.json && tsc -p tsconfig.supabase-ssr.main.json",
    "postinstall": "bun run supabase:ssr:build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/ssr": "github:jtclarkjr/ssr",
    "@supabase/supabase-js": "^2.86.0",
    "@tanstack/react-query": "^5.90.11",
    "@types/ioredis": "^5.0.0",
    "@upstash/redis": "^1.35.7",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "framer-motion": "^12.23.25",
    "ioredis": "^5.8.2",
    "ky": "^1.14.0",
    "lucide-react": "^0.555.0",
    "next": "16.0.7",
    "next-themes": "^0.4.6",
    "prettier": "^3.7.4",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "sonner": "^2.0.7",
    "swagger-ui-react": "^5.30.3",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.9"
  },
  "resolutions": {
    "prismjs": "^1.30.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "js-yaml": "^4.1.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/eslintrc": "^3.3.3",
    "@storybook/addon-a11y": "^10.1.4",
    "@storybook/addon-docs": "^10.1.4",
    "@storybook/addon-onboarding": "^10.1.4",
    "@storybook/addon-vitest": "^10.1.4",
    "@storybook/nextjs": "^10.1.4",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^24.10.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/swagger-ui-react": "^5.18.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.7",
    "eslint-plugin-storybook": "^10.1.4",
    "storybook": "^10.1.4",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
}
