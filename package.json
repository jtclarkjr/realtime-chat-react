{
  "name": "realtime-chat-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun run --bun next dev",
    "dev:instance1": "bun run --bun next dev -p 3000",
    "dev:instance2": "bun run --bun next dev -p 3001",
    "build": "bun run --bun next build",
    "build:app": "next build",
    "start": "next start",
    "lint": "eslint && tsc --noEmit",
    "prettier": "prettier --write .",
    "docker:up": "docker compose up --build -d",
    "docker:down": "docker compose down -v",
    "docker:logs": "docker compose logs -f redis",
    "docker:debug": "docker compose --profile debug up -d",
    "dev:docker": "docker compose up -d && bun dev",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-storybook-local": "storybook build && rm -rf public/storybook && mv storybook-static public/storybook",
    "supabase:ssr:build": "tsc -p tsconfig.supabase-ssr.module.json && tsc -p tsconfig.supabase-ssr.main.json",
    "postinstall": "bun run supabase:ssr:build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "github:jtclarkjr/ssr",
    "@supabase/supabase-js": "^2.93.2",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-virtual": "^3.13.18",
    "@tiptap/extension-code-block-lowlight": "^3.19.0",
    "@tiptap/extension-link": "^3.19.0",
    "@tiptap/react": "^3.19.0",
    "@tiptap/starter-kit": "^3.19.0",
    "@types/ioredis": "^5.0.0",
    "@upstash/redis": "^1.36.1",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.29.2",
    "ioredis": "^5.9.2",
    "ky": "^1.14.3",
    "lowlight": "^3.3.0",
    "lucide-react": "^0.563.0",
    "marked": "^17.0.1",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "prettier": "^3.8.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "sonner": "^2.0.7",
    "supabase": "^2.72.8",
    "swagger-ui-react": "^5.31.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.10"
  },
  "resolutions": {
    "qs": "^6.14.1",
    "tar": "^7.5.4",
    "lodash": "^4.17.23"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@storybook/addon-a11y": "^10.2.1",
    "@storybook/addon-docs": "^10.2.1",
    "@storybook/addon-onboarding": "^10.2.1",
    "@storybook/addon-vitest": "^10.2.1",
    "@storybook/nextjs": "^10.2.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^25.0.10",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "@types/swagger-ui-react": "^5.18.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-plugin-storybook": "^10.2.1",
    "storybook": "^10.2.1",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
}
