{
  "name": "realtime-chat-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:instance1": "next dev --turbopack -p 3000",
    "dev:instance2": "next dev --turbopack -p 3001",
    "build": "next build --turbopack",
    "build:app": "next build --turbopack",
    "start": "next start",
    "lint": "eslint && tsc --noEmit",
    "prettier": "prettier --write .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f redis",
    "docker:debug": "docker-compose --profile debug up -d",
    "dev:docker": "docker-compose up -d && bun dev",
    "redis:cli": "docker exec -it realtime-chat-redis redis-cli",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-storybook-local": "storybook build && rm -rf public/storybook && mv storybook-static public/storybook"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.63.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/react-query": "^5.90.2",
    "@types/ioredis": "^5.0.0",
    "@upstash/redis": "^1.35.4",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "framer-motion": "^12.23.22",
    "ioredis": "^5.8.0",
    "isomorphic-dompurify": "^2.28.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "next-themes": "^0.4.6",
    "prettier": "^3.6.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sonner": "^2.0.7",
    "swagger-ui-react": "^5.29.0",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.8"
  },
  "resolutions": {
    "prismjs": "^1.30.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/eslintrc": "^3.3.1",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/addon-vitest": "^9.1.10",
    "@storybook/nextjs": "^9.1.10",
    "@tailwindcss/postcss": "^4.1.13",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^20.19.17",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "@types/swagger-ui-react": "^5.18.0",
    "eslint": "^9.36.0",
    "eslint-config-next": "15.5.4",
    "eslint-plugin-storybook": "^9.1.10",
    "storybook": "^9.1.10",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.2"
  }
}
